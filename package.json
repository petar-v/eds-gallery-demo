{
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "seed": "npx ts-node -O '{\"module\":\"commonjs\"}' seed.ts",
        "cf-deploy": "npx @cloudflare/next-on-pages@1",
        "run-password-page": "npx wrangler pages dev build -b CFP_PASSWORD=test",
        "eslint": "eslint --fix",
        "stylelint": "stylelint '**/*.{css,scss,js,ts}'",
        "prettify": "prettier  --ignore-unknown --write .",
        "prettify:check": "prettier --check --ignore-unknown .",
        "prepare": "husky install",
        "precommit": "lint-staged"
